<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configureation 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>
	

	
	<definition name="*.*.*" template="/WEB-INF/views/{2}/template.jsp">
		<put-attribute name="asset" value="/WEB-INF/views/inc/asset.jsp"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/inc/header_{1}.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/{2}/{3}.jsp"></put-attribute>
		<put-attribute name="assetchart" value="/WEB-INF/views/inc/assetchart.jsp"></put-attribute>
	</definition>
	
			

	

	

	
</tiles-definitions>














